#include <iostream>
using namespace std;
int main(){
    int arr[]={2,3,5,3,11,3};
    int max=arr[0];
    int smax=arr[0];
    for(int i=0;i<6;i++){
        if(arr[i]>max){
            smax=max;
            max=arr[i];
        }
        else if(arr[i]<max && arr[i]>smax){
            smax=arr[i];
        }

    }
    cout<<smax;
}
